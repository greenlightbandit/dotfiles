<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="apps-accessories-menu" label="Accessories">
  <item label="Calculator">
    <action name="Execute">
      <command>calc</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Metadata Tagger">
	  <action name="Execute">
		  <command>easytag</command>
	  </action>
  </item>
  <item label="Atom">
	  <action name="Execute">
		  <command>atom</command>
	  </action>
  </item>
</menu>

<menu id="generic-school" label="School">
    <item label="Rstudio">
    <action name="Execute">
        <command>rstudio-bin</command>
    </action>
</item>
<item label="TexStudio">
    <action name="Execute">
        <command>texstudio</command>
    </action>
</item>
    <item label="Praat">
        <action name="Execute">
            <command>~/Programs/praat</command>
        </action>
    </item>
    <item label="JabRef">
        <action name="Execute">
            <command>jabref</command>
        </action>
    </item>
</menu>
        

