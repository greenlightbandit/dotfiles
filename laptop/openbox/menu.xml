<menu id="common-menu" label="Common">
 <item label="Firefox">
    <action name="Execute">
      <command>firefox</command>
      <startupnotify>
        <enabled>yes</enabled>
        <wmclass>Firefox</wmclass>
      </startupnotify>
    </action>
  </item>
<item label="Files">
    <action name="Execute">
	    <command>krusader</command>
    </action>
  </item>

  <item label="Music Player">
    <action name="Execute">
      <command>cmus</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

  
  <item label="Text Editor">
    <action name="Execute">
      <command>kate</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

  <item label="Email">
	  <action name="Execute">
		  <command>thunderbird</command>
		  <startupnotify>
			  <enabled>yes</enabled>
		  </startupnotify>
	  </action>
  </item>

  <item label="Terminal">
	  <action name="Execute">
		  <command>terminator</command>
	  </action>
  </item>
</menu>

<menu id="system-menu" label="System">
  <item label="Openbox Configuration Manager">
    <action name="Execute">
      <command>obconf</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <item label="Manage Cups Printers">
    <action name="Execute">
      <command>xdg-open http://localhost:631/</command>
      <startupnotify>
        <enabled>no</enabled>
        <icon>cups</icon>
      </startupnotify>
    </action>
  </item>
  <separator />
  <item label="Reconfigure Openbox">
    <action name="Reconfigure" />
  </item>
</menu>

<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  <menu id="common-menu"/>
  <menu id="generic-school"/>
  <menu id="apps-accessories-menu"/>
  <menu id="apps-graphics-menu"/>

  <menu id="apps-office-menu"/>
  <menu id="apps-multimedia-menu"/>
  <menu id="games-menu"/>
  <separator label="System" />
  <menu id="system-menu"/>
  <separator />
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
